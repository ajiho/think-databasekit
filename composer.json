{
  "name": "ajiho/think-databasekit",
  "type": "library",
  "description": "illuminate/database integration for thinkphp6",
  "keywords": [
    "database migrations",
    "migrations",
    "thinkphp",
    "illuminate",
    "laravel",
    "eloquent",
    "orm",
    "database",
    "sql"
  ],
  "homepage": "https://github.com/ajiho/think-databasekit",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ajiho\\databasekit\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "authors": [
    {
      "name": "ajiho",
      "email": "lujiahao@88.com",
      "homepage": "https://github.com/ajiho",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "composer-plugin-api": "^2.3",
    "nikic/php-parser": "^4.17",
    "vlucas/phpdotenv": "^4",
    "illuminate/events": "^7",
    "illuminate/console": "^7",
    "illuminate/filesystem": "^7",
    "fakerphp/faker": "^1.20",
    "illuminate/database": "^7.0",
    "topthink/framework": "^6.0",
    "ext-json": "*",
    "ajiho/illuminate-files-scoper": "^1.0"
  },
  "require-dev": {
    "composer/composer": "^2.3"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    },
    "think": {
      "services": [
        "ajiho\\databasekit\\Service"
      ],
      "config": {
        "databasekit": "src/config.php"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "ajiho/illuminate-files-scoper": true
    }
  }
}
